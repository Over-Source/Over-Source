macro OUTBINNAME "server"
macro OUTBINDIR "$SRC_DIR/../game/$GAMENAME/bin"
macro GAMENAME [!$GAMENAME]

include "$SRC_DIR/_qpc_scripts/source_dll_base.qpc"

configuration
{
	general
	{
		out_dir "./$CONFIG/$GAMENAME"
		
		include_directories
		{
			"$SRC_DIR/game/server"
			"$SRC_DIR/game/shared"
			"$SRC_DIR/utils/common"
			"$SRC_DIR/game/shared/econ"
			"$SRC_DIR/game/server/NextBot"
		}
	}
	
	compiler
	{
		preprocessor_definitions
		{
			"GAME_DLL"
			"VECTOR"
			"VERSION_SAFE_STEAM_API_INTERFACES"
			"PROTECTED_THINGS_ENABLE"
			"sprintf=use_Q_snprintf_instead_of_sprintf"
			"strncpy=use_Q_strncpy_instead"
			"_snprintf=use_Q_snprintf_instead"
			"fopen=dont_use_fopen"
		}
		
		precompiled_header "use"
		precompiled_header_file "cbase.h"
	}
	
	linker
	{
		libraries
		{
			"winmm.lib"
			"bonesetup"
			"choreoobjects"
			"dmxloader"
			"mathlib"
			"particles"
			"responserules_runtime"
			"tier2"
			"tier3"
			"vgui_controls"
			"steam_api"
		}
	}
}

